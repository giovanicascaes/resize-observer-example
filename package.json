    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.0.0",
    "postcss-import": "^12.0.1",
